<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/connect/db" VERSION="20140313" COMMENT="XMLDB file for Connect module"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="connect_course_dets" COMMENT="Each record is one courses connect details">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="campus" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="startdate" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="enddate" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="weeks" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="unlocked" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="locked_change_at" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="locked_change_by" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="true" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_course_chksum" COMMENT="Checksums for Moodle to SDS course sync">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Moodle Course ID"/>
        <FIELD NAME="module_delivery_key" TYPE="char" LENGTH="36" NOTNULL="true" SEQUENCE="false" COMMENT="SDS module_delivery_key"/>
        <FIELD NAME="session_code" TYPE="char" LENGTH="4" NOTNULL="true" SEQUENCE="false" COMMENT="SDS session_code"/>
        <FIELD NAME="chksum" TYPE="char" LENGTH="36" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="course_unique" TYPE="unique" FIELDS="courseid"/>
        <KEY NAME="sdsuid_unique" TYPE="unique" FIELDS="module_delivery_key, session_code"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="connect_enrolment_chksum" COMMENT="Checksums for Moodle to SDS enrolment sync">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="moodleid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Moodle Enrolment ID"/>
        <FIELD NAME="chksum" TYPE="char" LENGTH="36" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="connect_campus" COMMENT="Connect Campuses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="unique_name" TYPE="unique" FIELDS="name"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="connect_enrolments" COMMENT="Connect Enrolments">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="11" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="course" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="user" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="role" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="index_mid" UNIQUE="false" FIELDS="mid"/>
        <INDEX NAME="index_course" UNIQUE="false" FIELDS="course"/>
        <INDEX NAME="index_user" UNIQUE="false" FIELDS="user"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_group" COMMENT="Connect Groups">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="11" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="course" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="index_mid" UNIQUE="false" FIELDS="mid"/>
        <INDEX NAME="index_course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_group_enrolments" COMMENT="Connect Group Enrolments">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="11" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="user" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="index_mid" UNIQUE="false" FIELDS="mid"/>
        <INDEX NAME="index_groupid" UNIQUE="false" FIELDS="groupid"/>
        <INDEX NAME="index_user" UNIQUE="false" FIELDS="user"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_course" COMMENT="Connect Courses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="11" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="module_delivery_key" TYPE="char" LENGTH="36" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="session_code" TYPE="char" LENGTH="4" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="module_version" TYPE="char" LENGTH="4" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="campus" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="module_week_beginning" TYPE="char" LENGTH="4" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="module_length" TYPE="char" LENGTH="4" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="week_beginning_date" TYPE="datetime" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="module_title" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="module_code" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="synopsis" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="category" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="unique_module_delivery_key_session_code_module_version" TYPE="unique" FIELDS="module_delivery_key, session_code, module_version"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="index_mid" UNIQUE="false" FIELDS="mid"/>
        <INDEX NAME="index_module_delivery_key_session_code" UNIQUE="false" FIELDS="module_delivery_key, session_code"/>
        <INDEX NAME="index_category" UNIQUE="false" FIELDS="category"/>
        <INDEX NAME="index_module_code" UNIQUE="false" FIELDS="module_code"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_role" COMMENT="A list of roles known to connect">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="11" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="unique_name" TYPE="unique" FIELDS="name"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="index_mid" UNIQUE="false" FIELDS="mid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_user" COMMENT="Connect Users">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="11" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="ukc" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="login" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="initials" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="family_name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="unique_login" TYPE="unique" FIELDS="login"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="index_mid" UNIQUE="false" FIELDS="mid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="connect_course_links" COMMENT="A list of linked courses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="parent" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="The &quot;master&quot; ID"/>
        <FIELD NAME="child" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="A child of the parent"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="parent_child_key" TYPE="unique" FIELDS="parent, child"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="parent_idx" UNIQUE="false" FIELDS="parent"/>
        <INDEX NAME="child_idx" UNIQUE="false" FIELDS="child"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>